package de.schneckedde.grizzno
import com.soywiz.korev.Key

class KeyBindings(){
  private fun onKeyDown(key: Key) {
        when (key) {
            Key.W -> {
                println("W pressed")
                }
            
            Key.A -> {
                println("A pressed")
            }
            Key.S -> {
                println("S pressed")
                }
            Key.D -> {
                println("D pressed")
                }
                }
                }
                }
                
